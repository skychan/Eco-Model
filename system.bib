@Article{Moore2006,
  Title                    = {Business ecosystems and the view from the firm},
  Author                   = {Moore, James F},
  Year                     = {2006},
  Pages                    = {31},
  Volume                   = {51},
  Abstract                 = {For more than sixty years, markets and hierarchies have dominated people's thinking about economic organization. Markets, hierarchies, and ecosystems are the three pillars of modern business thinking and should provide the foundation for competition policy, regulation, and antitrust actions. The focus of companies in most sectors has progressed from competing on efficiency and effectiveness to competing on the basis of continuous innovation. Courts and regulators need to recognize the ecosystem form, appreciate its nature, structure, and operation, and seek to support its contributions to procompetitive and proinnovative social outcomes. The business ecosystem organizational form solves the problem of how to open up and extend a framework of modularity and participation. Markets facilitate transactions for goods. Hierarchies facilitate control over activities that produce goods. Ecosystems facilitate coordination of innovation in goods and the activities that produce them, as well as facilitate managed co-evolution of the complex web of markets and hierarchies themselves.},
  File                     = {:产业生态系统\\Business ecosystems and the view from the firm.pdf:PDF},
  Journal                  = {Antitrust Bull.},
  Keywords                 = {Innovations, Competition, Antitrust, Origanizational behavior, Business community},
  Owner                    = {sky},
  Publisher                = {HeinOnline},
  Timestamp                = {2016.02.28}
}

